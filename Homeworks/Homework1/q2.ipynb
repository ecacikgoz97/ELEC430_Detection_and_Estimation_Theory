   "source": [
    "f(x) = Λ*exp(-Λ*x);\n",
    "function f2(x, α=2)\n",
    "    if x > α\n",
    "        out = Λ*exp((-Λ*x)+α)\n",
    "    else\n",
    "        out = 0\n",
    "    end\n",
    "    return out\n",
    "end\n",
    "      "
   ]
